<p id="notice"><%= notice %></p>

<h1>Stock Values</h1>

<table>
  <thead>
    <tr>
      <th>Ticker</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stock_values.each do |stock_value| %>
      <tr>
        <td><%= stock_value.ticker %></td>
        <td><%= link_to 'Show', stock_value %></td>
        <td><%= link_to 'Edit', edit_stock_value_path(stock_value) %></td>
        <td><%= link_to 'Destroy', stock_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock Value', new_stock_value_path %>
